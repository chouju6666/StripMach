<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="Username">使用者</system:String>
    <system:String x:Key="Password">密码</system:String>
    <system:String x:Key="close">关闭</system:String>
    <system:String x:Key="French">French</system:String>
    <system:String x:Key="English">English</system:String>
    <system:String x:Key="Exit">离开</system:String>
    <system:String x:Key="Login">登入</system:String>
</ResourceDictionary>